# Създаване на речник с кодоните за аминокиселините
кодони_аминокиселини = {
    "GUU": "Валин",
    "GUC": "Валин",
    "GUA": "Валин",
    "GUG": "Валин",
    "CGU": "Аргинин",
    "CGC": "Аргинин",
    "CGA": "Аргинин",
    "CGG": "Аргинин",
    "AGA": "Аргинин",
    "AGG": "Аргинин",
    # Добавете останалите кодони и аминокиселини тук...
}

# Входна последователност от кодони (например)
кодони = ["GUU", "CGU", "GUC", "AGA", "CGC", "GUA", "CGG", "GUG", "AGG", "CGA"]

# Декодиране на последователността
декодирани_аминокиселини = [кодони_аминокиселини[кодон] for кодон in кодони]

# Извеждане на резултата
print("Декодирана последователност от аминокиселини:")
for аминокиселина in декодирани_аминокиселини:
    print(аминокиселина)
